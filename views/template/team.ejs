
<div class="container mt-4">
    <div class="row">
        <% arrayEquipo.forEach(trabajador => { %>
            <div class="col-3 mt-4">
                <div class="card">
                    <h3 class="card-header text-center"><%= trabajador.ocupacion %></h3>
                    <div class="card-body">
                        <img src="img/trabajadores/<%= trabajador._id %>/imgC.jpg" id="imgTrabajador" alt="" class="img-card img-fluid rounded-circle mb-4">
                        <h4 class="card-Nombre text-center"><%= trabajador.nombre %> <%= trabajador.app %></h4>
                        <p class="card-info text-center text-muted"><%= trabajador.carrera %></p>
                        <hr>
                        <p class="card-txt card-email ">Correo Electr√≥nico: <%= trabajador.email %></p>
                        <p class="card-txt card-email ">Sucursal: <%= trabajador.sucursal %></p>
                        <hr>
                        <p class="redes text-center">
                            <i class="bi bi-twitter text-dark mx-3"></i>
                            <i class="bi bi-facebook text-dark mx-3"></i>
                            <i class="bi bi-linkedin text-dark mx-3"></i>
                            <i class="bi bi-instagram text-dark mx-3"></i>
                        </p>
                    </div>
                </div>
            </div>
        <% }) %>
    </div> 
    <div class="row mt-2">
        <nav aria-label="Page navigation example">
            <ul class="pagination pagination-md justify-content-end">
              <li class="page-item"><a class="page-link" href="#">Previous</a></li>
              <li class="page-item"><a class="page-link" href="#">1</a></li>
              <li class="page-item"><a class="page-link" href="#">2</a></li>
              <li class="page-item"><a class="page-link" href="#">3</a></li>
              <li class="page-item"><a class="page-link" href="#">Next</a></li>
            </ul>
        </nav>
    </div>
    
</div>

